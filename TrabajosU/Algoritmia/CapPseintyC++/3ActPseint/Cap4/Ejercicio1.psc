Algoritmo Ejercicio1
	Definir num, serie Como Entero;
	Escribir "***Programa para escribir diez veces la serie de numeros del 1 al 10***";
	serie <- 0;
	Mientras serie < 10 Hacer
		num <- 1;
		Mientras num <= 10 Hacer
			Escribir num;
			num <- num + 1;
		Fin Mientras
		serie <- serie + 1;
	Fin Mientras
FinAlgoritmo
