Algoritmo ejercicio4
	Escribir "***Programa para calcular el area de una circunferencia***";
	Definir radio, area Como Real;
	Escribir "Escriba el valor del radio de la circunferencia (en centimetros): ", sin saltar;
	Leer radio;
	area <- PI * radio*radio;
	Escribir "El area de la circunferencia es: ", area;
	
FinAlgoritmo
