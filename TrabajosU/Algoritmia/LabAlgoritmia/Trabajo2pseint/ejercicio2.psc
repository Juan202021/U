Algoritmo ejercicio2
	Escribir "***Programa para calcular el promedio de las notas***";
	Definir n1, n2, n3, n4, suma, prom Como Real;
	Escribir "Digite la primera nota: ", sin saltar;
	Leer n1;
	Escribir "Digite la segunda nota: ", sin saltar;
	Leer n2;
	Escribir "Digite la tercera nota: ", sin saltar;
	Leer n3;
	Escribir "Digite la cuarta nota: ", sin saltar;
	Leer n4;
	suma <- n1+n2+n3+n4;
	prom <- suma/4;
	Escribir "El promedio de notas es: ", prom;
FinAlgoritmo
